<html>

	<head>
		<meta charset="utf-8"/>
		<meta http-equiv="x-ua-compatible" content="ie=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1"/>
		<title>
			{% block title %}Hibuk{% endblock %}
		</title>
		{% block stylesheets %}
			{% stylesheets filter='less,cssrewrite,scssphp'
			  'bundles/hibukcore/less/layout.less'
			  output='css/styles.css'
			%}
				<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
			{% endstylesheets %}
		{% endblock %}
		<script src="{{ asset('bundles/hibukcore/js/vendor/modernizr-custom.min.js') }}"></script>
	</head>

	<body>
		<header class="clearfix">
			<div id="entete">
				<a href="#">
					<img id="hibuk_logo" src="{{ asset('bundles/hibukcore/images/identity/logo.svg') }}" alt="Hibuk"/>
					<img id="hibuk_name" src="{{ asset('bundles/hibukcore/images/identity/nom.svg') }}" alt="Hibuk"/>
				</a>
			</div>
			<nav>
				<ul>
					<li><a href="#">{{ 'header.what' | trans({}, 'layout') }}</a></li>
					<li><a href="#">{{ 'header.estimate' | trans({}, 'layout') }}</a></li>
					<li><a id="connexion" href="#">{{ 'header.connect' | trans({}, 'layout') }}</a></li>
				</ul>
			</nav>
		</header>

		<main>
			{% block body %}{% endblock %}
		</main>

		<script src="https://code.jquery.com/jquery-3.1.0.min.js" integrity="sha256-cCueBR6CsyA4/9szpPfrX3s49M9vUU5BgtiJj06wt/s="
		crossorigin="anonymous"></script>
		{% block javascript %}{% endblock %}
	</body>
</html>